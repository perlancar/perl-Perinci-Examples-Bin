#!/usr/bin/perl

# VERSION

# since Perinci::CmdLine requires us for testing, if we don't have
# Perinci::CmdLine yet, we exit 0 so we don't fail compile tests.
eval { require Perinci::CmdLine }; exit 0 if $@;

Perinci::CmdLine->new(url=>'/Perinci/Examples/Completion/fruits')->run;

# ABSTRACT: A script to test completing array argument's elements
# PODNAME: peri-eg-complete-fruits

=head1 SYNOPSIS

 % complete -C peri-eg-complete-fruits peri-eg-complete-fruits
 % peri-eg-complete-fruits <tab>
 % peri-eg-complete-fruits apple <tab>
 % peri-eg-complete-fruits apple banana <tab>
 % peri-eg-complete-fruits apple banana b<tab>
 # and so on

=cut
